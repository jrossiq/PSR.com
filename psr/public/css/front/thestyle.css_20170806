}
.pull-rigth {
  float: right;
}
.flex-row {
  display: flex;
  /* Safari */
  flex-direction: row;
}
.flex-column {
  display: flex;
  /* Safari */
  flex-direction: column;
}
.flex-none {
  flex: 0 0 auto;
  -webkit-flex: 0 0 auto;
}
.flex-xs {
  flex: 1 0 0;
  -webkit-flex: 1 0 0;
}
.flex-sm {
  flex: 2 0 0;
  -webkit-flex: 2 0 0;
}
.flex-md {
  flex: 3 0 0;
  -webkit-flex: 3 0 0;
}
.flex-lg {
  flex: 4 0 0;
  -webkit-flex: 4 0 0;
}
