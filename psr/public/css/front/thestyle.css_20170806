.alpha-on {
  visibility: visible;
  opacity: 1;
}
.alpha-off {
  visibility: hidden;
  opacity: 0;
}
.img-hflip {
  transform: scaleX(-1);
  filter: FlipH;
  -ms-filter: "FlipH";
}
.list-unstyled {
